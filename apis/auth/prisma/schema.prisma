generator client {
  provider        = "prisma-client-js"
  engineType      = "library"
  previewFeatures = ["postgresqlExtensions"]
  output          = "../node_modules/@bms/auth-prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id   Int    @id @default(autoincrement())
  name String @unique
}
